{
    "name": "synergy/synergydatagrid",
    "description": "Module to display data in grid using the jqGrid javascript plugin",
    "type": "synergy-module",
    "license": "BSD-3-Clause",
    "keywords": [
        "data grid",
        "jqgrid",
        "zf2",
        "module"
    ],
    "homepage": "http://developer.peleodiase.com/synergy-data-grid",
    "authors": [
        {
            "name": "Pele Odiase",
            "email": "peleodiase@yahoo.co.uk"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": ">=2.0",
        "doctrine/doctrine-orm-module": ">=0.5",
        "doctrine/doctrine-module": ">=0.7",
        "monolog/monolog": "1.7.*",
        "ewgo/solarium-module": "dev-master",
        "zf-commons/zfc-user-doctrine-orm": "0.*",
        "synergy/common": "dev-master",
        "bjyoungblood/bjy-authorize": "dev-master"
    },
    "suggest": {
        "gedmo/doctrine-extensions": ">=v2.2.0"
    },
    "autoload": {
        "psr-0": {
            "SynergyDataGrid": "src/",
            "SynergyDataGridTest": "tests/module"
        },
        "classmap": [
            "./Module.php",
            "tests/module/Module.php"
        ]
    }
}
